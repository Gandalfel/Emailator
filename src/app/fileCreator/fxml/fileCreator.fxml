<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="helpButton" layoutX="530.0" layoutY="360.0" mnemonicParsing="false" onMouseClicked="#helpButtonClicked" prefWidth="60.0" text="Help" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="10.0" />
        <Button fx:id="cancelButton" layoutX="460.0" layoutY="360.0" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" prefWidth="60.0" text="Cancel" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="80.0" />
        <Button fx:id="finishButton" layoutX="390.0" layoutY="360.0" mnemonicParsing="false" onAction="#finishButtonClicked" prefWidth="60.0" text="Finish" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="150.0" />
        <Label layoutX="15.0" layoutY="15.0" text="Name:" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <Label layoutX="15.0" layoutY="80.0" text="Path:" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="80.0">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <TextField fx:id="nameTextField" layoutX="15.0" layoutY="35.0" prefHeight="25.0" prefWidth="400.0" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="35.0" />
        <TextField fx:id="pathTextField" layoutX="15.0" layoutY="105.0" prefHeight="25.0" prefWidth="400.0" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="105.0" />
        <Label fx:id="errorLabel" layoutX="16.0" layoutY="159.0" prefHeight="17.0" prefWidth="400.0" textFill="#dd1111" />
      <Button fx:id="openFileChooser" layoutX="392.0" layoutY="105.0" mnemonicParsing="false" onMouseClicked="#openFileChooser" prefHeight="25.0" prefWidth="30.0" text="..." AnchorPane.leftAnchor="392.0" AnchorPane.topAnchor="105.0" />
    </children>
</AnchorPane>
